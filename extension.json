                        if (API.getDJ() !== undefined || API.getWaitList().length !== 0) return;


                        window.wluInt = setTimeout(() => {
                                if (API.getDJ() !== undefined || API.getWaitList().length !== 0) return;


                                getCurrentLive((media) => {
                                        isUnavailable(media, (unavailable, media) => {
